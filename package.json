{
  "name": "buildabrainrot",
  "version": "1.0.0",
  "description": "Build a Brainrot - Create and share silly brainrot characters",
  "scripts": {
    "create-placeholders": "node scripts/create-placeholders.js",
    "generate-images": "node --dns-result-order=ipv4first scripts/generate-images.js",
    "generate-audio": "node --dns-result-order=ipv4first scripts/generate-audio.js",
    "generate-all": "npm run generate-images && npm run generate-audio",
    "generate-batch": "node --dns-result-order=ipv4first scripts/generate-images.js --batch",
    "validate-assets": "bash scripts/validate-assets.sh"
  },
  "dependencies": {
    "replicate": "^0.34.1",
    "dotenv": "^16.4.5",
    "sharp": "^0.33.5"
  },
  "devDependencies": {},
  "author": "",
  "license": "MIT"
}
